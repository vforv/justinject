{
  "name": "justinject",
  "version": "1.0.11",
  "description": "Light DI for typescript",
  "main": "dist/index",
  "typings": "dist/index",
  "typescript": {
    "definition": "dist/index"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage",
      "test",
      "example",
      "src/Model.ts"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  },
  "scripts": {
    "build": "tsc --traceResolution",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "dev": "nodemon",
    "test": "nyc --check-coverage --lines 100 --functions 100 --branches 100 --statements 100 mocha -r ts-node/register test/**/*.test.ts",
    "ts-node": "npm run lint && ts-node ./example"
  },
  "author": "Vladimir Djukic <www.vladimirdjukic.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/vforv/justinject"
  },
  "license": "ISC",
  "dependencies": {
    "@types/node": "~6.0.46",
    "@types/boom": "^7.2.0",
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "boom": "^7.2.0",
    "boomify": "0.0.1",
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.3",
    "nyc": "^11.7.1",
    "reflect-metadata": "^0.1.12",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3"
  },
  "devDependencies": {
    "@types/reflect-metadata": "^0.1.0"
  }
}
